# Variable declare Hol jarunk a listben
hol_jarunk = 0
#Variable declare Hany darab van az adott elembol
hany_darab = 1
#List amibe a stringbol teszem az elemeket
l = []
#Dictionary
dic = {}
# Split string to list
for word in s.split(' '):
    l.append(word)

#Vegigmenni  a listen egy loopban
while hol_jarunk <= len(l):
    #Szerepel-e a szó többször
    x = str (l[hol_jarunk])
    for l[l[hol_jarunk]] in l:
        dic [x] = hany_darab
        hany_darab += 1
        l.remove(x)
        hol_jarunk += 1
        
        #Ha igen --> annyivel valtoztatni a hany darabot
            #Dictionarybe menteni
        #Ha nem --> Dictionary-be menteni
    #Novelni a variablet ami azt szamolja hogy hol jarunk a listben
for key, value in dict.dic():
    print key
    print value
